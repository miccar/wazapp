{
	"name": "ws-custom-client",
	"version": "3.0.0",
	"description": "Whatsapp Custom Client for Linux",
	"main": "index.js",
	"scripts": {
		"start": "electron .",
		"build-linux64": "electron-packager . wscc --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/icon.png --prune=true --out=release",
		"build-linux32": "electron-packager . wscc --overwrite --asar --platform=linux --arch=ia32 --icon=assets/icons/icon.png --prune=true --out=release",
		"package-linux": "electron-packager . wscc --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/miccar/wazapp/"
	},
	"keywords": [
		"Electron",
		"WhatsApp",
		"linux",
		"debian",
		"ubuntu",
		"fedora",
		"WhatsApp works with Google Chrome 36+",
		"WhatsApp custom client"
	],
	"license": "CC0-1.0",
	"devDependencies": {
		"electron": "^6.0.12"
	},
	"dependencies": {
		"electron-packager": "^14.0.6"
	}
}
